{
  "compilerOptions": {
    "allowJs": false,
    "baseUrl": ".",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": [
      "es6",
      "dom",
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "paths": {
      "/*": [
        "*",
      ],
    },
    "sourceMap": true,
    "strict": true,
  },
  "exclude": [
    ".cache",
    ".vscode",
    "dist",
  ],
}
