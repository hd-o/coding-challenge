{
  "compilerOptions": {
    "isolatedModules": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "strict": true,
    "module": "commonjs",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "sourceMap": true,
    "allowJs": false,
    "lib": [
      "es6",
      "dom"
    ],
  },
  "exclude": [
    ".vscode",
    "dist",
    ".cache"
  ]
}