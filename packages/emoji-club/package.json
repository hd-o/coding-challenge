{
  "name": "emoji-club",
  "description": "Full Stack Emoji NFT Dapp",
  "version": "0.1.0",
  "private": true,
  "keywords": [
    "ethereum",
    "nft",
    "react"
  ],
  "scripts": {
    "dev": "concurrently 'yarn:dev:*'",
    "dev:ethereum": "cd packages/ethereum && yarn dev",
    "dev:web": "cd packages/web && yarn dev",
    "lint": "concurrently 'yarn:lint:*'",
    "lint:ethereum": "cd packages/ethereum && yarn lint",
    "lint:web": "cd packages/web && yarn lint"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "concurrently": "^7.2.2"
  }
}
