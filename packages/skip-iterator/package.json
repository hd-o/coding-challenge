{
  "name": "skip-iterator",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "run-s setup start:*",
    "lint": "run-s setup:eslint run:eslint",
    "run:eslint": "eslint -c .eslintrc .",
    "run:jest": "DEBUG_PRINT_LIMIT=0 jest",
    "sandbox": "npx --yes pnpm i && npx pnpm sandbox:start",
    "sandbox:start": "run-p start:parcel jest",
    "setup": "run-p setup:*",
    "setup:eslint": "node .config/eslint",
    "setup:jest": "node .config/jest",
    "setup:typescript": "node .config/typescript",
    "start": "run-s start:parcel",
    "start:parcel": "parcel index.html",
    "test": "run-s setup run:jest",
    "test:watch": "run-s setup:jest 'run:jest -- --watch'"
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.11",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}