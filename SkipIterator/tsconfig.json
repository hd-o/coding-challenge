{
  "exclude": [
    "node_modules/",
    ".cache/",
    "dist/"
  ],
  "compilerOptions": {
    "target": "ES5",
    "strict": true,
    "module": "commonjs",
    "jsx": "preserve",
    "esModuleInterop": true,
    "sourceMap": true,
    "allowJs": true,
    "lib": [
      "es6",
      "dom"
    ],
    "rootDir": "src",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}